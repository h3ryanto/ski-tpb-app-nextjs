model Entitas {
id	                      BigInt @id @default(autoincrement())
nomor_aju                 String    
header                    Header @relation(fields: [nomor_aju], references: [nomor_aju], onDelete: Cascade)        
seri	                    Int? 
kode_entitas	            String? 
kode_jenis_identitas	    String? 
nomor_identitas	          String? 
nama_entitas	            String? 
alamat_entitas	          String? 
nib_entitas	              String? 
kode_jenis_api	          String? 
kode_status	              String? 
nomor_ijin_entitas	      String? 
tanggal_ijin_entitas	    String? 
kode_negara	              String? 
niper_entitas	            String? 
kode_kategori_konsolidator String?
kode_afiliasi              String?
created_at		            DateTime @default(now())		
updated_at		            DateTime @updatedAt
@@index([nomor_aju])
}

